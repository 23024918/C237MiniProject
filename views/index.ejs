<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>ISLP Dashboard</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  <style>
    body {
      background-color: #f8f9fa;
    }
    .sidebar {
      width: 200px;
      height: 100vh;
      background-color: #343a40;
      position: fixed;
      top: 0;
      left: 0;
      padding-top: 20px;
    }
    .sidebar a {
      display: block;
      color: #fff;
      padding: 16px;
      text-decoration: none;
    }
    .sidebar a:hover {
      background-color: #495057;
    }
    .main-content {
      margin-left: 200px;
      padding: 40px;
    }
    .dashboard-title {
      text-align: center;
      font-weight: bold;
      margin-bottom: 40px;
    }
    .dashboard-box {
      background-color: #d9d9d9;
      border-radius: 20px;
      height: 100px;
      margin-bottom: 20px;
    }
  </style>
</head>
<body>
  <!-- Sidebar -->
  <div class="sidebar">
    <a href="/">Home</a>
    <a href="/addTicket">Add New Ticket</a>
    <a href="/contact">Contact Us</a>
    <a href="/feedback">Feedback</a>
  </div>

  <!-- Main Content -->
  <div class="main-content">
    <h2 class="dashboard-title">ISLP</h2>
    <div class="dashboard-box w-100"></div>
    <div class="dashboard-box w-100"></div>
    <div class="dashboard-box w-100"></div>
    <div class="dashboard-box w-100"></div>
  </div>
</body>
</html>


